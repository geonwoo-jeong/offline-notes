{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "max-classes-per-file": false,
    "no-console": false,
    "jsx-no-lambda": false,
    "no-unused-expression": [true, "allow-tagged-template"]
  },
  "linterOptions": {
    "exclude": ["node_modules/**/*.ts"]
  }
}
